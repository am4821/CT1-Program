#include <stdio.h>
void tHanoi(int n,char from_rod,char to_rod,char aux_rod){
    if(n==0)
    return;
    tHanoi(n-1,from_rod,aux_rod,to_rod);
    printf("Move disk %d from rod %c to rod %c\n",n,from_rod,to_rod);
    tHanoi(n-1,aux_rod,to_rod,from_rod);
}
int main()
{
int n;
scanf("%d",&n);
tHanoi(n,'A','C','B');
	return 0;
}
